@startuml
!pragma teoz true


actor Paciente as P
participant "Servicio Web Pacientes" as SW

activate P
activate SW

P -> SW : PedirCitaRemoto(DNI)
SW --> P :  resguardodecita

P -> SW :  PedirCambioDeCitaRemoto(DNI)
SW --> P :  resguardodecita

P -> SW : AnularCitaRemoto(DNI)
SW --> P :  confirmacion

P -> SW :  ValoracionRemota(DNI, Valoracion)
SW --> P :  confirmacion





@enduml